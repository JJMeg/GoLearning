package main

import (
	"fmt"
	_ "fmt"
)

func main() {
	var personAge map[string]int
	personAge = make(map[string]int)

	personAge["amy"] = 1
	personAge["Daming"] = 2

	fmt.Println(personAge)

	for name, age := range personAge {
		fmt.Println(name, age)
	}

	delete(personAge, "amy")
	fmt.Println(personAge)
}
