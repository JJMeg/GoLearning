package main

import (
	"fmt"
	_ "fmt"
)

func main() {
	const (
		a = 1
		b = 2
		c = 3
	)
	if (a < b) {
		if (b < 5) {
			fmt.Println("a<b && b<5")
		}
	} else if (c == 3) {
		fmt.Println("c==3")
	} else {
		fmt.Println("noattachable")
	}

	switch a {
	case 1:
		fmt.Println("a==1")
		break
	case 2:
		fmt.Println("a==2")
		break
	default:
		break
	}
}
