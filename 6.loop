package main

import (
	"fmt"
	_ "fmt"
)

func main() {
	var i int = 1;
	for true {
		i ++
		if (i > 10) {
			fmt.Println("loop1: ", i)
			break
		}
	}

	for j := 0; j < 10; j++ {
		fmt.Println("loop2: ", j)
	}

	for i < 20 {
		i++
		fmt.Println("loop3: ", i)
	}

	number := [6]int{1, 2, 3, 4, 5, 6}
	for k, x := range number {
		fmt.Println("loop4: index %d: %d", k, x)
	}

}
